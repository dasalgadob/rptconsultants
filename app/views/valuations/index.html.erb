<p id="notice"><%= notice %></p>
<%= render 'layouts/breadcrumb', titles: [['Menu',menu_path]]%>

<h1>Valuations</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Job title</th>
      <th>Position type</th>
      <th>Knowledge</th>
      <th>Skill</th>
      <th>Definition supervision</th>
      <th>Risk decision</th>
      <th>Sustainability</th>
      <th>Area impact</th>
      <th>Influence</th>
      <th>Score</th>
      <th>Degree</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @valuations.each do |valuation| %>
      <tr>
        <td><%= valuation.job_title %></td>
        <td><%= valuation.position_type %></td>
        <td><%= valuation.knowledge %></td>
        <td><%= valuation.skill %></td>
        <td><%= valuation.definition_supervision %></td>
        <td><%= valuation.risk_decision %></td>
        <td><%= valuation.sustainability %></td>
        <td><%= valuation.area_impact %></td>
        <td><%= valuation.influence %></td>
        <td><%= valuation.score %></td>
        <td><%= valuation.degree %></td>
        <td><%= link_to 'Show', valuation %></td>
        <td><%= link_to 'Edit', edit_valuation_path(valuation) %></td>
        <td><%= link_to 'Destroy', valuation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Valuation', new_valuation_path %>
